<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chatbot.web.mappers.UserMapper">
    <select id="selectId" parameterType="int" resultType="string">
        select user_id as userId
        from user
        where user_code = 100020001
    </select>

    <select id="selectPw" parameterType="int" resultType="string">
        select user_pw as userPw
        from user
        where user_code = 100020001
    </select>
</mapper>